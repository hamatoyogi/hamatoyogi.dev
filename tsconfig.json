{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2022",
    "moduleResolution": "bundler",
    "paths": {
      "~components/*": ["src/components/*"],
      "~assets/*": ["src/assets/*"],
      "~style/*": ["src/style/*"],
      "~layouts/*": ["src/components/layouts/"],
      "~typography/*": ["src/components/typography/"],
      "~types/*": ["src/types/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik"
  }
}
